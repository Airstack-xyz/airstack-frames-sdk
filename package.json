    "@types/node": "^20.11.17",
    "@types/node-fetch": "^2.6.11",
    "@types/react": "^18.2.73",
    "@types/sha256": "^0.2.2",
    "@types/uuid": "^9.0.8",
    "@vercel/node": "^3.0.26",
    "dotenv": "^16.4.1",
    "jest": "^29.7.0",
    "next": "^14.1.4",
    "protobufjs": "^7.2.6",
    "ts-jest": "^29.1.2",
